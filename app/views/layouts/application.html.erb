  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="http://connect.soundcloud.com/sdk.js"></script>
	<script>
		SC.initialize({
		  client_id: 'f43e5fe0023f09c558e18747e7c4c708'
		});

